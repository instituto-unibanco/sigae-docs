{
  "name": "sigae-apidocs",
  "version": "0.0.1",
  "dependencies": {
    "@redocly/cli": "^1.16.0"
  },
  "private": true,
  "scripts": {
    "start": "redocly preview-docs",
    "join-windows": "./combine.ps1",
    "join": "redocly join ./_info.yaml ./sigae_apis/*.yaml -o dist/bundle.yaml",
    "join-linux": "./combine.sh",
    "lint": "redocly lint dist/bundle.yaml",
    "build": "redocly build-docs dist/bundle.yaml -o docs/index.html"
  }
}
